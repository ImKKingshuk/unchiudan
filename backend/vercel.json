{
  "rewrites": [
    {
      "source": "/public/(.*)",
      "destination": "/public/$1"
    },
    {
      "source": "/:path*",
      "destination": "/",
      "has": [
        {
          "type": "path",
          "value": "/public/(.*)"
        },
        {
          "type": "not"
        }
      ]
    }
  ]
}
